    <sql>SELECT 
cs.customer_id As custId ,
qgi.date_conducted As createdDate ,
qgr.response As response ,
qgr.question_group_instance_id As instanceId ,
qgr.sections_questions_id As qid
FROM 
question_group_instance qgi 
JOIN question_group qg ON qg.id = qgi.question_group_id AND qgi.event_source_id = 3 AND qg.id = 4
JOIN question_group_response qgr ON qgr.question_group_instance_id = qgi.id
JOIN customer cs ON cs.customer_id = qgi.entity_id
ORDER BY instanceId</sql>
