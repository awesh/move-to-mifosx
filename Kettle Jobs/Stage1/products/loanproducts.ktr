    <sql>SELECT
  po.PRD_OFFERING_ID As id
, ${CUR} As currency_code
, ${CURDIGITS} As currency_digits
, laa.DEFAULT_LOAN_AMOUNT As principal_amount
, laa.MIN_LOAN_AMOUNT As min_principal_amount
, laa.MAX_LOAN_AMNT As max_principal_amount
, 0 As arrearstolerance_amount
, po.PRD_OFFERING_NAME As name
, po.DESCRIPTION As description
, null  As fund_id
, lo.DEF_INTEREST_RATE As 	nominal_interest_rate_per_period
, lo.MIN_INTEREST_RATE As 	min_nominal_interest_rate_per_period
, lo.MAX_INTEREST_RATE As 	max_nominal_interest_rate_per_period
, 3 As 	interest_period_frequency_enum -- Per year
, lo.DEF_INTEREST_RATE As 	annual_nominal_interest_rate
, lo.INTEREST_TYPE_ID As 	interest_method_enum
, 1 As 	interest_calculated_in_period_enum  -- Same as repayment period
, rd.RECUR_AFTER As 	repay_every
, rd.RECURRENCE_ID As 	repayment_period_frequency_enum
, nl.DEFAULT_NO_INSTALL As 	number_of_repayments
, nl.MIN_NO_INSTALL As 	min_number_of_repayments
, nl.MAX_NO_INSTALL As 	max_number_of_repayments
, if(lo.GRACEPERIOD_TYPE_ID = 2 OR lo.GRACEPERIOD_TYPE_ID = 3 , lo.GRACE_PERIOD_DURATION , NULL ) As 	grace_on_principal_periods
, if(lo.GRACEPERIOD_TYPE_ID = 2 , lo.GRACE_PERIOD_DURATION , NULL ) As 	grace_on_interest_periods
, NULL As 	grace_interest_free_periods
, 1 As 	amortization_method_enum
, 1 As 	accounting_type
, 1 As 	loan_transaction_strategy_id -- Mifos Style
, po.GLOBAL_PRD_OFFERING_NUM As 	external_id
, loan_counter_flag As include_in_borrower_cycle
, prd.start_date As start_date
, if(prd.end_date &lt;&gt; NULL , prd.end_date , if(prd.offering_status_id = 1 , NULL , prd.updated_date ))  As close_date
FROM prd_offering po 
JOIN loan_offering lo ON lo.PRD_OFFERING_ID = po.PRD_OFFERING_ID
JOIN prd_offering prd ON prd.prd_offering_id = lo.prd_offering_id
LEFT JOIN loan_amount_same_for_all_loan laa ON laa.PRD_OFFERING_ID = lo.PRD_OFFERING_ID
JOIN prd_offering_meeting pom ON pom.PRD_OFFERING_ID = lo.PRD_OFFERING_ID
JOIN meeting me ON me.MEETING_ID = pom.PRD_MEETING_ID
JOIN recurrence_detail rd ON rd.MEETING_ID = me.MEETING_ID
JOIN no_of_install_same_for_all_loan nl ON  nl.PRD_OFFERING_ID = lo.PRD_OFFERING_ID
</sql>
